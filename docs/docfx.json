{
  "metadata": [
    {
      "src": [
        {
          "cwd": "..",
          "files": [
            "src/Moq/Moq.csproj"
          ]
        }
      ],
      "filter": "filterConfig.yml",
      "dest": "api",
      "properties": {
        "TargetFramework": "netstandard2.0"
      },
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appLogoPath": "images/logo.png",
      "_appName": "Moq"
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "template"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ],
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}