{
  "version": "1.0.0-*",
  "description": "Moq.NetCore.UnitTests Class Library",
  "authors": [ "" ],
  "packOptions": {
    "tags": [ "" ],
    "projectUrl": "",
    "licenseUrl": ""
  },

  "buildOptions": {
    "keyFile": "../../Moq.snk",
    "compile": {
      "include": [
        "../../UnitTests/**/*.cs"
      ],
      "exclude": [
        "../../UnitTests/Library/**/*.cs",
        "../../UnitTests/Pex/**/*.cs",
        "../../UnitTests/Properties/**/*.cs"
      ]
    }
  },

  "dependencies": {
    "xunit": "2.1.0"
  },

  "testRunner": "xunit",
  "frameworks": {
    "netcoreapp1.0": {
      "buildOptions": {
        "define": [ "NETCORE" ]
      },
      "dependencies": {
        "Castle.Core": "4.0.0-beta001",
        "ClassLibrary1": {
          "target": "project",
          "version": "1.0.0-*"
        },
        "ClassLibrary2": {
          "target": "project",
          "version": "2.0.0-*"
        },
        "Moq": {
          "target": "project",
          "version": "99.99.99-wrapped"
        },
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0"
        },
        "System.Collections.Specialized": "4.0.1",
        "System.ComponentModel.Primitives": "4.1.0",
        "System.Diagnostics.TraceSource": "4.0.0",
        "System.ServiceModel.Primitives": "4.1.0",
        "dotnet-test-xunit": "2.2.0-preview2-build1029"
      },
      "imports": [ "dnxcore50", "portable-net451+win81" ]
    }
  }
}
